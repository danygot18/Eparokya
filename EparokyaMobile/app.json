{
  "expo": {
    "name": "Eparokya",
    "slug": "Eparokya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "*/"
    ],
    "plugins": [
      [
        "expo-notifications",
        {
          "mode": "production",
          "ios": {
            "allowAlert": true,
            "allowSound": true,
            "allowBadge": true
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.danrussell.frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "978594fd-9d13-416b-b7dc-01449c529690"
      }
    }
  }
}